{"mode":"Text","textContent":"#region VEXcode Generated Robot Configuration\nfrom vex import *\nimport urandom\n\n# Brain should be defined by default\nbrain=Brain()\n\n# Robot configuration code\n\n\n# wait for rotation sensor to fully initialize\nwait(30, MSEC)\n\n\ndef play_vexcode_sound(sound_name):\n    # Helper to make playing sounds from the V5 in VEXcode easier and\n    # keeps the code cleaner by making it clear what is happening.\n    print(\"VEXPlaySound:\" + sound_name)\n    wait(5, MSEC)\n\n# add a small delay to make sure we don't print in the middle of the REPL header\nwait(200, MSEC)\n# clear the console to make sure we don't have the REPL in the console\nprint(\"\\033[2J\")\n\n#endregion VEXcode Generated Robot Configuration\nimport time\nimport math\nimport random\n\ndef roundup(x):\n    return int(math.ceil(x / 5.0)) * 5\n# Robot configuration\ncontroller_1 = Controller(PRIMARY)\n# drivetrain\ndt_left = MotorGroup(Motor(Ports.PORT19, GearSetting.RATIO_6_1, True), Motor(Ports.PORT20, GearSetting.RATIO_6_1, True))\ndt_right = MotorGroup(Motor(Ports.PORT11, GearSetting.RATIO_6_1, False), Motor(Ports.PORT12, GearSetting.RATIO_6_1, False))\n\ndt_left.set_stopping(BRAKE)\ndt_right.set_stopping(BRAKE)\n# inertial & solenoid\norientation = Inertial(Ports.PORT1)\nwings = DigitalOut(brain.three_wire_port.a)\n# components\nintake = Motor(Ports.PORT8, GearSetting.RATIO_18_1, False)\nendgame = MotorGroup(Motor(Ports.PORT15, GearSetting.RATIO_18_1, False), Motor(Ports.PORT16, GearSetting.RATIO_18_1, True))\ncatapult = Motor(Ports.PORT9, GearSetting.RATIO_18_1, False)\n\nintake.set_velocity(200, PERCENT)\nendgame.set_position(0,DEGREES)\nendgame.set_stopping(BRAKE)\n# initialize everything\norientation.calibrate()\nclass Components:\n    def __init__(self, endgame_ratio, cata_speed):\n        self.endgame_ratio = endgame_ratio\n        catapult.set_velocity(cata_speed, PERCENT)\n        self.wing_value = False\n        self.intake_value = None\n        wings.set(False)\n    def intake(self, direction):\n        if direction == None:\n            intake.stop()\n        else:\n            intake.spin(direction)\n        self.intake_value = direction\n    def endgame(self, direction, amount=None):\n        endgame.set_velocity(200, PERCENT)\n        if direction == None:\n            endgame.stop()\n        elif amount == None:\n            endgame.spin(direction)\n        else:\n            d = FORWARD if ((1 if direction == FORWARD else -1)*(1 if amount>=0 else -1)) == 1 else REVERSE\n            endgame.spin_for(d, abs(amount)*self.endgame_ratio, TURNS)\n    def catapult(self, direction=FORWARD, amount=None):\n        if direction == None:\n            catapult.stop()\n        elif amount == None:\n            catapult.spin(direction)\n        else:\n            d = FORWARD if ((1 if direction == FORWARD else -1)*(1 if amount>=0 else -1)) == 1 else REVERSE\n            catapult.spin_for(d, abs(amount)*self.endgame_ratio, TURNS)\n    def wings(self):\n        self.wing_value = False if self.wing_value else True\n        wings.set(self.wing_value)\nclass Drivetrain:\n    def __init_(self, gear_ratio, wheel_diameter):\n        self.gear_ratio = gear_ratio\n        self.wheel_diameter = wheel_diameter\n    def _exact_dispatch(self, left_amount, right_amount, left_speed, right_speed, timeout):\n        dt_left.set_velocity(left_speed, PERCENT)\n        dt_right.set_velocity(right_speed, PERCENT)\n\n        dt_left.set_timeout(timeout, SECONDS)\n        dt_right.set_timeout(timeout, SECONDS)\n        \n        dt_left_dir = FORWARD if left_amount >= 0 else REVERSE\n        dt_right_dir = FORWARD if right_amount >= 0 else REVERSE\n        dt_left.spin_for(dt_left_dir, (abs(left_amount)/(math.pi*3.25))*60/36, TURNS, wait=False)\n        dt_right.spin_for(dt_right_dir, (abs(right_amount)/(math.pi*3.25))*60/36, TURNS, wait=False)\n        old_time = brain.timer.time(SECONDS)\n        while dt_left.is_spinning() or dt_right.is_spinning():\n            if brain.timer.time(SECONDS)-old_time >= timeout:\n                dt_left.stop()\n                dt_right.stop()\n                break\n    def drive4(self, inches, speed=100, timeout=3):\n        # # self._exact_dispatch(inches, inches, speed, speed, timeout)\n        # direction = FORWARD if inches > 0 else REVERSE\n        # target = (abs(inches)/(math.pi*3.25))*60/36\n\n        # dt_left.spin(direction)\n        # dt_right.spin(direction)\n        # old_time = brain.timer.time(SECONDS)\n\n        # while (brain.timer.time(SECONDS) - old_time <= timeout) and (abs(dt_left.position(TURNS)-target)) >= 0.2 or abs(dt_right.position(TURNS)-target)) >= 0.2):\n        #     dt_left_velocity = speed * abs(dt_left.position(TURNS)-target)/target * \n        #     dt_left.set_velocity(speed, PERCENT)\n        #     dt_right.set_velocity(speed, PERCENT)\n        \n        # dt_left.spin_for(direction, , TURNS, wait=False)\n        # dt_right.spin_for(direction, (abs(inches)/(math.pi*3.25))*60/36, TURNS, wait=False)\n        # old_time = brain.timer.time(SECONDS)\n        # while dt_left.is_spinning() or dt_right.is_spinning():\n        #     if brain.timer.time(SECONDS)-old_time >= timeout:\n        #         dt_left.stop()\n        #         dt_right.stop()\n        #         break\n        dt_left.set_timeout(timeout)\n        dt_right.set_timeout(timeout)\n        dt_left.set_velocity(speed, PERCENT)\n        dt_right.set_velocity(speed, PERCENT)\n        direction = FORWARD if inches > 0 else REVERSE\n        dt_left.spin_for(direction, (abs(inches)/(math.pi*3.25))*60/36, TURNS, wait=False)\n        dt_right.spin_for(direction, (abs(inches)/(math.pi*3.25))*60/36, TURNS, wait=False)\n        old_time = brain.timer.time(SECONDS)\n        while dt_left.is_spinning() or dt_right.is_spinning():\n            if brain.timer.time(SECONDS)-old_time >= timeout:\n                dt_left.stop()\n                dt_right.stop()\n                break\n    def turn2(self, angle_unmodded, speed=41, tolerance=0.5):\n        angle = angle_unmodded % 360\n        while abs(angle - orientation.heading(DEGREES)) % 360 > tolerance:\n            wait(10, MSEC)\n            h = orientation.heading(DEGREES)\n            dt_left.set_velocity(abs((angle - h + 180) % 360 - 180) * speed * 2 / 180 + 3, PERCENT)\n            dt_right.set_velocity(abs((angle - h + 180) % 360 - 180) * speed * 2 / 180 + 3, PERCENT)\n            dt_left.spin(FORWARD if (angle - h + 180) % 360 - 180 > 0 else REVERSE)\n            dt_right.spin(REVERSE if (angle - h + 180) % 360 - 180 > 0 else FORWARD)\n        dt_left.stop()\n        dt_right.stop()\n        if abs(angle - orientation.heading(DEGREES)) % 360 > tolerance:\n            self.turn2(angle, speed=10, tolerance=tolerance)\n    def experimental_arc(self, angle, radius, check=False, speed=100, timeout=3):\n        # we have to use differential driving to efficiently arc\n        direction = radius\n        #left_amount = (angle/360)*(2*math.pi*(radius-))\n        # self._exact_dispatch(left_amount, right_amount, speed, speed, timeout)\n        pass\n\ndt = Drivetrain(60/36, 3.25)\ncp = Components(80/12, 65)\nwhile orientation.is_calibrating():\n    wait(10, MSEC)\nbrain.timer.clear()\n\n# driver control\ndef driver_control():\n    controller_1.buttonL2.pressed(cp.wings)\n    controller_1.buttonR2.pressed(cp.wings)\n\n    controller_1.buttonL1.pressed(lambda: cp.intake(None if cp.intake_value == FORWARD else FORWARD))\n    controller_1.buttonR1.pressed(lambda: cp.intake(None if cp.intake_value == REVERSE else REVERSE))\n\n    controller_1.buttonUp.pressed(lambda: cp.endgame(FORWARD))\n    controller_1.buttonDown.pressed(lambda: cp.endgame(REVERSE))\n    controller_1.buttonUp.released(endgame.stop)\n    controller_1.buttonDown.released(endgame.stop)\n\n    controller_1.buttonX.pressed(cp.catapult)\n    controller_1.buttonB.released(catapult.stop)\n\n    controller_1.buttonA.pressed(lambda: dt.turn2(sorted([0, 90, 180, 270, 360], key=lambda x: abs(x-orientation.heading(DEGREES)))[0]))\n    controller_1.buttonY.pressed(lambda: dt.turn2(sorted([0, 90, 180, 270, 360], key=lambda x: abs(x-orientation.heading(DEGREES)))[0]))\n    cata_speed=75\n    cata_acceleration=0\n    while True:\n        if controller_1.buttonRight.pressing():\n            cata_acceleration += 0.025\n        elif controller_1.buttonLeft.pressing():\n            cata_acceleration -= 0.025\n        else:\n            cata_acceleration = 0\n        cata_speed += cata_acceleration\n\n        controller_1.screen.set_cursor(1, 1)\n        controller_1.screen.print(int(cata_speed))\n\n        catapult.set_velocity(cata_speed, PERCENT)\n        \n        dt_left_speed = controller_1.axis3.position() + 0.5*controller_1.axis1.position()\n        dt_left.set_velocity(abs(dt_left_speed), PERCENT)\n        dt_left_dir = FORWARD if dt_left_speed >= 0 else REVERSE\n\n\n        dt_right_speed = controller_1.axis3.position() - 0.5*controller_1.axis1.position()\n        dt_right.set_velocity(abs(dt_right_speed), PERCENT)\n        dt_right_dir = FORWARD if dt_right_speed >= 0 else REVERSE\n        \n        dt_left.spin(dt_left_dir)\n        dt_right.spin(dt_right_dir)\n\n        wait(50, MSEC)\n\ndef autonomous():\n    # preloads - one will be in the intake, the other used as a preload per <RSC2>\n    orientation.set_heading(90, DEGREES)\n    catapult.set_velocity(75, PERCENT)\n    initial_time = brain.timer.time(MSEC)\n\n    dt.drive4(-12, speed=75)\n    dt.turn2(-10, speed=75)\n    dt.drive4(-17, speed=75)\n    catapult.spin(FORWARD)\n    time.sleep(23.5)\n    catapult.stop()\n    dt.drive4(3, speed=75)\n    dt.turn2(45)\n    dt.drive4(17, speed=75)\n    dt.turn2(0)\n    dt.drive4(43, speed=75)\n    dt.turn2(0)\n    dt.drive4(43)\n    # smash 1 (side of goal)\n    dt.turn2(-45)\n    dt.drive4(26)\n    dt.turn2(-90)\n    dt.drive4(10, speed=140)\n    # smash 2 (angle 1)\n    dt.drive4(-6)\n    dt.turn2(0)\n    dt.drive4(-44)\n    dt.turn2(-40)\n    cp.wings()\n    dt.drive4(50)\n    # smash 3 (front of goal)\n    cp.wings()\n    dt.drive4(-3)\n    dt.turn2(0)\n    dt.drive4(-20)\n    dt.turn2(0)\n    cp.wings()\n    dt.drive4(30)\n    # smash 4 (angle 2)\n    dt.drive4(-3)\n    cp.wings()\n    dt.turn2(45)\n    dt.drive4(-35)\n    dt.turn2(40)\n    cp.wings()\n    dt.drive4(50, timeout=2)\n    # retreat!\n    cp.wings()\n    dt.drive4(-24, speed=150)\n    print(brain.timer.time(MSEC)-initial_time)\n    # switch to driver control (CONSIDER: climb)\n\ncompetition = Competition(driver_control, autonomous)\n","textLanguage":"python","rconfig":[],"slot":0,"platform":"V5","sdkVersion":"20220726.10.00.00","appVersion":"3.0.4","minVersion":"3.0.0","fileFormat":"1.2.0","icon":"","targetBrainGen":"First","v5Sounds":[{"name":"game over","url":"static/sounds/mixkit-arcade-retro-game-over-213.wav"}],"v5SoundsEnabled":false,"target":"Physical"}
